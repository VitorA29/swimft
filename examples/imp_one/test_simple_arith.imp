# simple test the basic arith operations
let cons x = 3+(4-5)*2, cons y = 3<4, cons h = 3<4 == False
in
    print(x)
    print(y)
    print(h)
    print(2+x*5)
    print(x+4)
    let cons z = x==1, var j = 2+x*5
    in
        print(z)
        print(j)
        let var w = 3>4 or z, cons k=y and z
        in
            print(w)
            print(k)
            print(j/7)
        end
    end
end
        